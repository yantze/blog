{
  "name": "hexo-site",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "hexo generate",
    "clean": "hexo clean",
    "deploy": "hexo deploy",
    "server": "hexo server",
    "sync": "yuque-hexo sync",
    "clean:yuque": "yuque-hexo clean"
  },
  "hexo": {
    "version": "7.3.0"
  },
  "yuqueConfig": {
    "postPath": "source/_posts",
    "cachePath": "yuque.json",
    "mdNameFormat": "slug",
    "adapter": "hexo",
    "concurrency": 5,
    "baseUrl": "https://www.yuque.com/api/v2",
    "login": "yantze",
    "repo": "blog",
    "onlyPublished": true,
    "onlyPublic": false,
    "lastGeneratePath": "lastGeneratePath.log",
    "imgCdn": {
      "enabled": true,
      "concurrency": 10,
      "imageBed": "oss",
      "region": "oss-cn-hongkong",
      "bucket": "vastiny-com",
      "prefixKey": "_imgs"
    }
  },
  "devDependencies": {
    "hexo": "^7.3.0",
    "hexo-deployer-ali-oss-extend": "^2.0.0",
    "hexo-generator-archive": "latest",
    "hexo-generator-category": "^2.0.0",
    "hexo-generator-i18n": "^0.0.10",
    "hexo-generator-index": "^4.0.0",
    "hexo-generator-tag": "^2.0.0",
    "hexo-hide-posts": "^0.4.3",
    "hexo-renderer-ejs": "^2.0.0",
    "hexo-renderer-marked": "^7.0.0",
    "hexo-renderer-stylus": "^3.0.1",
    "hexo-server": "^3.0.0",
    "hexo-theme-landscape": "^1.0.0",
    "yuque-hexo": "^1.9.5"
  },
  "dependencies": {
    "hexo-browsersync": "^0.3.0",
    "hexo-generator-feed": "^3.0.0",
    "hexo-generator-sitemap": "^3.0.1",
    "hexo-renderer-pug": "^3.0.0"
  }
}
